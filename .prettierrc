{
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "trailingComma": "all",
  "singleQuote": true,
  "bracketSpacing": true,
  "importOrder": [
    "^assets/(.*)$",
    "^pages/(.*)$",
    "^components/(.*)$",
    "^types/(.*)$",
    "^hooks/(.*)$",
    "^themes/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderCaseInsensitive": false
}
